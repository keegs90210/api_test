  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": false,
          "cmd": "",
          "name": "binaries/dist/main",
          "sidecar": true
        }
      ]
    },
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://**/"
        },
        {
          "url": "https://**/"
        }
      ]
    },
    "shell:default",
    "http:default"
  ]
}
